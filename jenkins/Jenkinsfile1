pipeline{
    agent any
    
    stages{
        stage('build'){
            steps{
                sh '''
                bash ${WORKSPACE}/New_script.sh
                echo "Build is ready"
                '''
            }
        }
        stage('test'){
            steps{
                sh '''
                echo "Test is ready"
                '''
            }
        }
        stage('deploy'){
            steps{
                sh '''
                echo "deploy is ready"
                '''
            }
        }
    }
}
